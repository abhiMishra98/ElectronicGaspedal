<?xml version="1.0" encoding="utf-8"?>
<rte>
 <configset>
  <config name="version" value="Erika 2"/>
  <config name="path_out" value="{PROJECT_PATH}\out"/>
  <config name="templateDir" value="{GEN_PATH}\..\templates\Erika"/>
  <config name="userCodeStart" value="/\* USER CODE START {TAG} \*/"/>
  <config name="userCodeEnd" value="/\* USER CODE END {TAG} \*/"/>
  <config name="guivers" value="0.1.2"/>
 </configset>
 <naming>
  <config name="signalpoolPrefix" value="sp_"/>
  <config name="signalpoolPostfix" value=""/>
  <config name="taskPrefix" value="tsk_"/>
  <config name="taskPostfix" value=""/>
  <config name="signalclassPrefix" value="sc_"/>
  <config name="signalclassPostfix" value=""/>
  <config name="signalobjectPrefix" value="so_"/>
  <config name="signalobjectPostfix" value=""/>
  <config name="swcPrefix" value="swc_"/>
  <config name="swcPostfix" value=""/>
  <config name="runnablePrefix" value=""/>
  <config name="runnablePostfix" value="_run"/>
  <config name="eventPrefix" value="ev_"/>
  <config name="eventPostfix" value=""/>
  <config name="resourcePrefix" value="res_"/>
  <config name="resourcePostfix" value=""/>
  <config name="addrPrefix" value="ADR_"/>
  <config name="addrStartPostfix" value="_START"/>
  <config name="addrEndPostfix" value="_END"/>
 </naming>
 <signalpools>
  <signalpool description="" endaddr="" name="sp_signalPool" shortname="signalPool" startaddr=""/>
 </signalpools>
 <tasks>
  <task description="Task having runnables which either receives input from the user or provide output to the actuators. Running runnables in 10ms and 100ms." events="ev_joystick_onData|ev_10ms|ev_100ms" mode="Event" name="tsk_IO" shortname="IO" signalpoolsRO="" signalpoolsRW="" tickEvent="0" timertickperiod="1"/>
  <task description="Task containing control runnable" events="ev_joystick_onData|ev_controlspeed_onData" mode="Event" name="tsk_Control" shortname="Control" signalpoolsRO="" signalpoolsRW="" tickEvent="0" timertickperiod="1"/>
 </tasks>
 <signalclasses>
  <signalclass description="signal is updated every 10ms and is used by the control logic to compute the vehicle's desired speed and behavior" name="sc_joystick" shortname="joystick"/>
  <signalclass description="signal is updated every time the control logic (run_calcControl) processes new joystick data" name="sc_speed" shortname="speed"/>
 </signalclasses>
 <signalobjects>
  <signalobject description="SO speed value forwarded to engine and brakelight" indriver="default" name="so_speed" onDataError="0" onDataUpdate="ev_controlspeed_onData" outdriver="0" resource="None" shortname="speed" signalclass="sc_speed" signalpool="sp_signalPool"/>
  <signalobject description="SO for transmitting joystick data between SWCs" indriver="default" name="so_joystick" onDataError="0" onDataUpdate="ev_joystick_onData" outdriver="default" resource="None" shortname="joystick" signalclass="sc_joystick" signalpool="sp_signalPool"/>
 </signalobjects>
 <swcs>
  <swc description="Software component" name="swc_joystick" shortname="joystick"/>
  <swc description="Software component" name="swc_calc_control" shortname="calc_control"/>
  <swc description="Software component" name="swc_engine_control" shortname="engine_control"/>
  <swc description="Software component" name="swc_brakelight_control" shortname="brakelight_control"/>
 </swcs>
 <runnables>
  <runnable component="swc_joystick" cycletime="100" description="Runnable" events="ev_10ms" name="JOYSTICK_Joystick_readJoystick_run" shortname="Joystick_readJoystick" signalIN="" signalOUT="so_joystick" task=""/>
  <runnable component="swc_calc_control" cycletime="100" description="Runnable" events="ev_joystick_onData" name="CALC_CONTROL_Calccontrol_calcControl_run" shortname="Calccontrol_calcControl" signalIN="so_joystick" signalOUT="" task=""/>
  <runnable component="swc_engine_control" cycletime="100" description="Runnable" events="ev_100ms" name="ENGINE_CONTROL_Engine_Control_setEngine_run" shortname="Engine_Control_setEngine" signalIN="" signalOUT="" task=""/>
  <runnable component="swc_brakelight_control" cycletime="100" description="Runnable" events="ev_controlspeed_onData" name="BRAKELIGHT_CONTROL_setBrakeLight_run" shortname="setBrakeLight" signalIN="" signalOUT="" task=""/>
 </runnables>
 <events>
  <event description="A Event" name="ev_joystick_onData" shortname="joystick_onData"/>
  <event description="A Event" name="ev_controlspeed_onData" shortname="controlspeed_onData"/>
  <event description="A Event" name="ev_10ms" shortname="10ms"/>
  <event description="A Event" name="ev_100ms" shortname="100ms"/>
 </events>
 <resources/>
</rte>
